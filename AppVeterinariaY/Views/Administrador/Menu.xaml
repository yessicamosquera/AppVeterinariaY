<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppVeterinariaY.Views.Administrador.Menu" 
             Title="Master"
             Background="#12a89d" >
    
    <ContentPage.Content Background="#12a89d">
        <StackLayout Background="white" >

            <Button Margin="0,60,0,0" Background="#12a89d" x:Name="RegistroEmTxt" Text="Registrar empleado" Clicked="RegistroEmTxt_Clicked"/>
            <Button Margin="0,20,0,0" Background="#12a89d" x:Name="VerEmplTxt" Text="Empleados" Clicked="VerEmplTxt_Clicked"/>
            <Button Margin="0,200,0,0" Background="#12a89d" Text="Cerrar sesion"
                    CornerRadius="10"
                    x:Name="salir"
                    Clicked="salir_Clicked"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>